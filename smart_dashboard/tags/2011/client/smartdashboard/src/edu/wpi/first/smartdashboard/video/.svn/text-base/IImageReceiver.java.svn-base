package edu.wpi.first.smartdashboard.video;

import java.awt.Image;

/**
 * An interface for objects that would like to be notified when a new image
 * arrives from the robot.
 * 
 * @author pmalmsten
 */
public interface IImageReceiver {
    /**
     * This method is called whenever a new image is available.
     *
     * @param img The recently received image.
     */
    void update(Image img);
}
